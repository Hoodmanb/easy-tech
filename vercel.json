{
    "version": 2,
    "builds": [
        {
            "src": "backend/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "client/build"
            }
        }
    ],
    "routes": [
        {
            "src": "/catalog/(.*)",
            "dest": "/backend/app.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "client/build/index.html"
        }
    ]
}